					<CheatEntry>
					  <ID>6004</ID>
					  <Description>"Set All Items To 1 Quantity (Use To Fix Key Items)"</Description>
					  <VariableType>Auto Assembler Script</VariableType>
					  <AssemblerScript>[ENABLE]
{$lua}
local MainAddr = ReadQword(getAddress("MainBaseAddr"))
if (MainAddr ~= nil) then
  local InventoryPtr1 = ReadQword(MainAddr + 0x3A8)
  local NumInventoryItems = ReadInteger(MainAddr + 0x3B0)

  if (InventoryPtr1 ~= nil) then
	  local CurrentItemIDNbr = 0
	  local CurrentItemID = ReadInteger(InventoryPtr1 + (CurrentItemIDNbr * 0x10))
	
	  while(CurrentItemIDNbr &lt; NumInventoryItems) do
	    WriteInteger(InventoryPtr1 + (CurrentItemIDNbr * 0x10)+0x04,1)

	    CurrentItemIDNbr = CurrentItemIDNbr + 1
	    CurrentItemID = ReadInteger(InventoryPtr1 + (CurrentItemIDNbr * 0x10))
	  end
  end
end  
{$asm}

