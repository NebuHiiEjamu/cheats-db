					  <VariableType>Auto Assembler Script</VariableType>
					  <AssemblerScript>[ENABLE]
{$lua}

local AddItemQty = ReadInteger(getAddress("AddItemQty"))
local AddItemID = ReadInteger(getAddress("AddItemID"))
local MainAddr = ReadQword(getAddress("MainBaseAddr"))
if (MainAddr ~= nil) then
local Char1Ptr1 = ReadQword(MainAddr + 0x9D50)

	if (Char1Ptr1 ~= nil) then
		local Char1Ptr2 = ReadQword(Char1Ptr1 + 0x68)

		if (Char1Ptr2 ~= nil) then
			if (AddItemID ~= 0xFFFF) then
				WriteInteger(Char1Ptr2 + (AddItemID * 0x18),AddItemID)
				WriteInteger(Char1Ptr2 + (AddItemID * 0x18) + 0x04,AddItemQty)
				WriteInteger(Char1Ptr2 + (AddItemID * 0x18) + 0x08,AddItemQty)
			end
		end
	end  
end

