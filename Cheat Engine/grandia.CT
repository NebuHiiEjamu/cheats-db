					  <Description>"Damage Modifier (God Mode, One Hit Kill, etc.)"</Description>
					  <Options moHideChildren="1"/>
					  <VariableType>Auto Assembler Script</VariableType>
					  <AssemblerScript>[ENABLE]


aobscanmodule(DamageModAOB,"grandia.exe",2B C1 0F B7 D1 66 89 86 00 01 00 00)
registersymbol(DamageModAOB)
alloc(newmem,2048,DamageModAOB)

///
label(originalcode)
label(return)
label(GodMode)
registersymbol(GodMode)
label(OneHitKill)
registersymbol(OneHitKill)
label(EnemyDamageMultiplier)
registersymbol(EnemyDamageMultiplier)
label(HeroDamageMultiplier)
registersymbol(HeroDamageMultiplier)
label(EnemyHealingMultiplier)
registersymbol(EnemyHealingMultiplier)
label(HeroHealingMultiplier)
registersymbol(HeroHealingMultiplier)
label(HeroCannotDie)
registersymbol(HeroCannotDie)
  
///
newmem:
  cmp byte ptr [esi+02],6
  jne EnemyHit
HeroHit:
  cmp ecx,0
  jge HeroDamageHit
  jmp originalcode
HeroDamageHit:
  cmp [HeroDamageMultiplier],(float)0.0
  je CheckGodMode
  push ecx
  fild dword ptr [rsp]
  fmul dword ptr [HeroDamageMultiplier]
  fistp dword ptr [rsp]
  pop ecx
CheckGodMode:
  cmp [GodMode],1
  jne CheckHeroCannotDie
  xor ecx,ecx
  jmp originalcode
CheckHeroCannotDie:
  cmp [HeroCannotDie],1
  jne originalcode
  cmp ecx,eax
  jl @F
  xor ecx,ecx
@@:
  jmp originalcode
EnemyHit:
  cmp ecx,0
  jge EnemyDamageHit
  jmp CheckOneHitKill
EnemyDamageHit:
  cmp [EnemyDamageMultiplier],(float)0.0
  je CheckOneHitKill
  push ecx
  fild dword ptr [rsp]
  fmul dword ptr [EnemyDamageMultiplier]
  fistp dword ptr [rsp]
  pop ecx
CheckOneHitKill:
  cmp [OneHitKill],1
  jne originalcode
  mov ecx,eax
  jmp originalcode

originalcode:
  sub eax,ecx
  movzx edx,cx
  jmp return

GodMode:
dq 0
OneHitKill:
dq 0
EnemyDamageMultiplier:
dq (float)0.0
HeroDamageMultiplier:
dq (float)0.5
EnemyHealingMultiplier:
dq (float)0.0
HeroHealingMultiplier:
dq (float)2.0
HeroCannotDie:
dq 1

DamageModAOB:
  jmp newmem
  return:
  
[DISABLE]

DamageModAOB:
  db 2B C1 0F B7 D1

unregistersymbol(GodMode)
unregistersymbol(OneHitKill)
unregistersymbol(EnemyDamageMultiplier)
unregistersymbol(HeroDamageMultiplier)
unregistersymbol(HeroCannotDie)
unregistersymbol(DamageModAOB)

dealloc(newmem)
