					  <AssemblerScript>[ENABLE]
{$lua}

local StashPtr = ReadPointer(getAddress("StashPtr"))
local ItemQuantity = ReadBytes(getAddress("ItemQuantity"))
local CurrentItem = 0

if (StashPtr ~= nil and StashPtr ~= 0) then
  while (CurrentItem &lt; (0x1FF - 0x09)) do
  -- Items Before Slot 9 Are Removed
    WriteBytes(StashPtr + 0x09 + CurrentItem,ItemQuantity)
    CurrentItem = CurrentItem + 1
  end
end

