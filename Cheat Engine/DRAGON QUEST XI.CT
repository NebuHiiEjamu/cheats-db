  fild dword ptr [rsp]
  fmul dword ptr [fHeroDamageMult]
  fistp dword ptr [rsp]
  pop esi
CheckGodMode:
  cmp [GodMode],1
  jne CheckHeroCannotDie
  xor esi,esi
  jmp CleanUp
CheckHeroCannotDie:
  cmp [HeroCannotDie],1
  jne CleanUp
  cmp esi,eax
  jl @F
  xor esi,esi
