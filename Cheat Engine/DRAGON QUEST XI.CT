  fmul dword ptr [fEnemyDamageMult]
  fistp dword ptr [rsp]
  pop esi
CheckOneHitKill:
  cmp [OneHitKill],1
  jne CleanUp
  mov esi,#99999
  jmp CleanUp

CleanUp:    
  cmp eax,esi
  jmp return

GodMode:
dq 0
OneHitKill:
dq 0
fEnemyDamageMult:
dq (float)0.0
fHeroDamageMult:
